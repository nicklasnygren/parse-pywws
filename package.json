{
  "name": "pywws-parse",
  "version": "0.1.1",
  "description": "Pywws data file parser for node.js",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && browserify --node -t babelify --no-bundle-external --standalone 'pywws-parse' src/index.js | uglifyjs -c > dist/main.js",
    "tap": "browserify --node -t babelify --no-bundle-external test/*.js | node",
    "test": "npm run tap | faucet",
    "watch": "nodemon -q -w ./src -w ./test -e js -x npm run tap",
    "prepublish": "npm run build"
  },
  "files": [
    "index.js",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicklasnygren/parse-pywws.git"
  },
  "keywords": [
    "pywws",
    "weather"
  ],
  "author": "Nicklas Nygren",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicklasnygren/parse-pywws/issues"
  },
  "homepage": "https://github.com/nicklasnygren/parse-pywws#readme",
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "bluebird": "^3.3.5",
    "csv": "^0.4.6",
    "iniparser": "^1.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "faucet": "0.0.1",
    "moment": "^2.12.0",
    "nodemon": "^1.9.1",
    "tape": "^4.5.1",
    "uglifyjs": "^2.4.10"
  }
}
